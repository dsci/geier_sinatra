<style type="text/css" media="screen">
	 @import "/stylesheets/fonts.css";
	h2{
		font: 30px/38px 'TitilliumText22LRegular', Arial, sans-serif;letter-spacing: 0;
	}
	.flyer_list{
		list-style:none;
		margin:0;
		padding:0;
		font: 18px/27px 'TitilliumText22LRegular', Arial, sans-serif;
	}
	
	.flyer_list a{
		color:blue;
		text-decoration:none;
	}
	
	.flyer_list li{
		background: url(../images/page_white_acrobat.png) no-repeat 0px 2px;
		padding-left:20px;
	}
</style>
<h2>Flyer</h2>
<ul class="flyer_list">
<%@flyers.each do |flyer| %>
	<li><a href="<%="#{@base_uri}/#{flyer}"%>"><%=flyer%></a></li>
<%end%>
</ul>